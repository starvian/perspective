        "@finos/perspective-viewer-datagrid": "^0.6.0",
        "@finos/perspective-workspace": "^0.6.0"
    },
    "devDependencies": {
        "@finos/perspective-webpack-plugin": "^0.6.0",
        "css-loader": "^0.28.7",
        "file-loader": "^4.2.0",
        "html-webpack-plugin": "^3.2.0",
        "http-server": "^0.11.1",
        "less-loader": "^4.0.5",
        "npm-run-all": "^4.1.3",
        "rimraf": "^2.5.2",
        "style-loader": "^0.18.2",
        "webpack-cli": "^3.3.7",
        "webpack-dev-server": "^3.8.0"
